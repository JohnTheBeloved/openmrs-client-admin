{
  "name": "openmrs-nodeclient-admin",
  "description": "Administrative Client Application for OpenMRS Webservices",
  "main": "index.js",
  "authors": [
    "JohnTheBeloved"
  ],
  "license": "MIT",
  "keywords": [
    "openmrs"
  ],
  "moduleType": [
    "node"
  ],
  "homepage": "public/index.html",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular-morris-chart": "~1.1.0",
    "font-awesome": "~4.5.0",
    "iCheck": "~1.0.2",
    "angular": "~1.4.8",
    "bower-jvectormap": "~1.2.2",
    "bootstrap-wysihtml5": "*",
    "jQuery": "~2.1.4",
    "jquery-ui": "~1.11.4",
    "sparkline": "~2.1.2",
    "gnob": "knob#~0.1.11",
    "bootstrap-datepicker": "~1.5.1",
    "slimScroll": "~1.3.7",
    "fastclick": "~1.0.6",
    "bootstrap-daterangepicker": "~2.1.17",
    "jquery-sparkline": "~2.1.3",
    "jquery-knob": "~1.2.13",
    "bootstrap": "3.3.5",
    "bootstrap3-wysihtml5-bower": "~0.3.3",
    "Ionicons": "ionicons#~2.0.1"
  },
  "exportsOverride": { 
    "bootstrap": {
      "js": "**/*.js",
      "css": "**/*.css",
      "fonts": "**/glyphicons-*.*" 
    },
    "font-awesome": {
      "fonts": "fonts/fontawesome*.*",
      "css": "css/font-awesome.css"
    },
    "Ionicons": {
      "fonts": "**/ionicons.*",
      "js": "**/ionicons.js",
      "css": "**/ionicons.css"
    },
    "*": {
      "js": "**/*.js",
      "css": "**/*.css",
      "fonts": "**/*"
    }
  },
  "resolutions": {
    "angular": ">= 1.0.8",
    "jquery": ">= 2.1.0"
  }
}
